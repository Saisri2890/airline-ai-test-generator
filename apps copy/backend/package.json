{
  "name": "airline-test-generator-backend",
  "version": "1.0.0",
  "description": "Backend for Airline AI Test Generator with Traditional Authentication",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "create-admin": "node scripts/create-admin.js",
    "setup": "npm install && npx prisma generate && npx prisma migrate dev && npm run create-admin",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.9",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "airline",
    "testing",
    "ai",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
  "type": "commonjs"
}
